class student
{
   int id,age;
   String name;
   student(int i,String n)
   {
       id=i;
       name=n;
   }
   student(int i,String n,int a)
   {
       id=i;
       name=n;
       age=a;
   }
   void display()
   {
       System.out.println(id+"-->"+name+"-->"+age);
   }
   public static void main(String args[])
   {
      student s1=new student(111,"sri",20);
      student s2=new student(222,"hema",18);
      s1.display();
      s2.display();
   }
}

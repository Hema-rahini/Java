import java.util.*;
class Reverse
{
    public static void main(String args[])
    {
        int i;
        Scanner obj=new Scanner(System.in);
        System.out.print("string:");
        String a=obj.nextLine();
        for(i=a.length()-1;i>=0;i--)
        {
            System.out.print(a.charAt(i));
        }
    }
}
